{
    "name": "clearhaus/sdk-zf",
    "description": "Clearhaus SDK integration into Zend Framework.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "clearhaus",
        "cloud",
        "payment",
        "transaction",
        "gateway",
        "api"
    ],
    "homepage": "https://github.com/RiskioFr/clearhaus-sdk-php-zf",
    "authors": [
        {
            "name": "Nicolas Eeckeloo",
            "email": "neeckeloo@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "clearhaus/sdk": "^0.1",
        "psr/container": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ClearhausModule\\": "src/"
        }
    },
    "extra": {
        "zf": {
            "config-provider": "ClearhausModule\\ConfigProvider",
            "module": "ClearhausModule\\Module"
        }
    }
}
